{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.19.4",
      "env": {
        "NODE_ENV": "development"
      },
      "prebuildCommand": "cd ../frontend-services && npm install && npm run build"
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "node": "20.19.4",
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "cd ../frontend-services && npm install && npm run build"
    },
    "production": {
      "autoIncrement": true,
      "node": "20.19.4",
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "prebuildCommand": "cd ../frontend-services && npm install && npm run build"
    }
  },
  "submit": {
    "production": {}
  }
}
